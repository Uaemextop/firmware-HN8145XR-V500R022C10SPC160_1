        "ahsapd.wifi",
        "ahsapd.roaming",
        "ahsapd.control",
        "ahsapd.mesh",
        "ahsapd.detection",
        "ahsapd.wifi.csi"
    ],
    "access": {
        "ahsapd.basic": {
            "methods": ["getBasicInfo"]
        },
        "ahsapd.hardware": {
            "methods": ["getHardware"]
        },
        "ahsapd.port": {
            "methods": ["getPortInfo"]
