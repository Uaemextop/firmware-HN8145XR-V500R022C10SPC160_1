            "methods": ["httpSpeedTest"]
        },
        "ahsapd.wifi.csi": {
            "methods": ["setCSIEnable", "getCSIEnable", "addCSIMac", "delCSIMac", "getCSIMac", "setCSIConfig", "getCSIConfig", "setCSIDriver", "getCSIDriver", "getCSIVersion", "setCSISpeed", "getCSISpeed", "setCSIScore", "setCSIDevice", "setCSIDevice"]
        },
        "ahsapd.qos": {
            "methods": ["setQosConfig", "getQosConfig"]
        },
        "ahsapd.vlan": {
            "methods": ["setIptvVlan", "getIptvVlan"]
        }
