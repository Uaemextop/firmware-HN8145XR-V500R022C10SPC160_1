            "methods": ["getUplinkInfo", "setPppoe", "wlanConnect","setWorkingMode","setUplinkRateConfig"]
        },
        "ahsapd.sta": {
            "methods": ["getStaInfo", "getStaNum", "setStaRateConfig"]
        },
        "ahsapd.wifi": {
            "methods": ["getWifiInfo", "setWifiParameter", "setWifiSwitch", "setRadioConfig", "setWps", "5GPreferred"]
        },
        "ahsapd.wifi.stats": {
            "methods": ["getWifiStatsInfo"]
