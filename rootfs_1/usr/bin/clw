#!/bin/busybox sh
#Çå³ýlastword

if [ 0 -ne $# ]; then
    echo "ERROR::input para is not right!";
    return 1;
else
    read -p "Confirm to clean the lastword(yes):" var_confirm
    if [ x$var_confirm == x"yes" ] ; then
        echo cls > /proc/wap_proc/wap_lastword
        return 0;
    else
        echo "Cancel to clean the lastword."
        return 1;
    fi
fi
