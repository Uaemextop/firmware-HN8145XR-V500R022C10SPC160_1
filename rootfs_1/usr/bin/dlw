#!/bin/sh
#²é¿´lastword

if [ 0 -eq $# ]; then
    cat /proc/wap_proc/wap_lastword;
    return 0;
elif [ 1 -eq $# ]; then  
    if [ "0" == $1 ]; then 
        [ -f /var/startup_lastword ] && zcat /var/startup_lastword;
        return 0;
    elif [ "1" == $1 ]; then 
        cat /proc/wap_proc/wap_lastword_1;
        return 0;
    elif [ "2" == $1 ]; then 
        cat /proc/wap_proc/wap_lastword_2;
        return 0;
    else
        echo "ERROR::input para is not right!";
        return 1;
    fi
else
    echo "ERROR::input para is not right!";
    return 1;
fi

