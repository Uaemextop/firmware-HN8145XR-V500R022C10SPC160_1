#!/bin/sh

paranum=$#

send_hlp()
{
	sndhlp $@ >>/dev/null
	result=$?
	if [ "$result" != "0" ];then
	    echo "ERROR::input para is not right!"
	fi
	
	return $result
}

send_hlp_check()
{
	sndhlp $@ >>/dev/null
	result=$?
	if [ "$result" != "0" ];then
	    echo "ERROR::not support!"
	fi
	
	return $result
}

send_hlp_print_log()
{
	sndhlp $@
	result=$?
	if [ "$result" != "0" ];then
	    echo "ERROR::input para is not right!"
	fi
	
	return $result
}

check_param()
{
	if [ "$paranum" != "$1" ];then
	  echo "ERROR::input para is not right!"
		return 1
	fi
	return 0
}

ishex()
{
	result=0
	case $1 in
	[0-f])
		result=1
		;;
	[0-f][0-f])
		result=1
		;;
	esac
	return $result
}

help_ploam()
{
		echo "         ploam"
		echo "                 -h  --- Get help of ploam Module"
		echo "                 print on  --- Open the printed switch of PLOAM messages" 
		echo "                 print off --- Close the printed switch of PLOAM messages"  
		echo "                 alarm on  --- Open the printed switch of PLOAM alarm messages"
		echo "                 alarm off  --- Close the printed switch of PLOAM alarm messages"
		echo "                 err on  --- Open the printed switch of PLOAM err messages" 
		echo "                 err off  --- Close the printed switch of PLOAM err messages"
		echo "                 omci on  --- Open the printed switch of OMCI messages"
		echo "                 omci off --- Close the printed switch of OMCI messages"
		echo "                 stat  --- get statistics of PLOAM messages" 
		echo "                 clear stat  --- Clear statistics of PLOAM messages" 
		echo "                 bwmap  --- Get BWMAP info" 
		echo "                 show log  --- display history register PLOAM message" 
		echo ""
}

help_lanploam()
{
		echo "         lanploam"
		echo "                 -h  --- Get help of lanploam Module"
		echo "                 print on  --- Open the printed switch of lanploam messages" 
		echo "                 print off --- Close the printed switch of lanploam messages"  
		echo ""
}

help_lanomci()
{
		echo "         lanomci"
		echo "                 -h  --- Get help of lanomci Module"
		echo "                 print on  --- Open the printed switch of lanomci messages" 
		echo "                 print off --- Close the printed switch of lanomci messages"  
		echo ""
}

help_optic()
{
		echo "         optic"
		echo "                 -h  --- Get help of optic Module"
		echo "                 display reg --- Get the value of optic register"
		echo ""
}

help_led()
{
		echo "         led"
		echo "                 -h  --- Get help of led Module"
		echo "                 display status [ledid] [ledcolor] --- display led status"
		echo ""
}


help_gpio()
{
		echo "         gpio"
		echo "                 -h  --- Get help of gpio Module"
		echo "                 display value [gpio]  ---Get value of gpio"
		echo ""
}

help_i2c()
{
		echo "         i2c"
		echo "                 -h  --- Get help of I2C Module"
		echo "                 read A0/A2/A4/A6 [addr] --- Get the value of A0/A2/A4/A6 register" 
		echo "                                       the "addr" expressed by hexadecimal represents the address of regiser,such as echo "6e"(don't use the "0x" before)"
		echo ""
}

help_usb()
{
		echo "         usb"
		echo "                 -h  --- Get help of USB Module"
		echo "                 display status --- Get the status of USB module"
		echo ""
}


help_codec()
{
		echo "         codec"
		echo "                 -h  --- Get help of Codec Module"
		echo "                 display global info [portid]     ---display the info of CODEC Module ,portid[0-3]"
		echo ""
}

help_phy()
{
		echo "         phy"
		echo "                 -h  --- Get help of Phy Module"
		echo "                 display debug info ---Get all phy register informations of ports"
		echo "                 display extphy info ---Get all extphy register informations of ports"
		echo ""
}

help_mpcp()
{
		echo "         mpcp"
		echo "                 switch on/off --- Open/Close the switch of MPCP"
		echo "                 display statistics --- Get statistics of MPCP"
		echo "                 show log --- display history register MPCP message"
		echo ""       
}                            
                              
help_wifi_statistic()         
{                             
		echo "             statistic"
		echo "                         -h                                                 --- Get help of Wifi Statistic Cmds"		                                                                                 
		echo "                         [ssid index]                                       --- display the statistics of ssid"
		echo "                                                                                index[if 4 SSIDs per radio,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo "                         clear [ssid index]                                 --- clear the statistics of ssid"
		echo "                                                                                index[if 4 SSIDs per radio,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo "                         alg [index]                                        --- display the channel log"
		echo "                                                                                index[ only 0 is used currently]"
		echo "                         sta [sta mac]                                      --- display the statistics of sta"
		echo "                         vap [ssid index]                                   --- display all sta's statistics of vap"
		echo "                                                                                index[if 4 SSIDs per radio,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo ""            
}                                  
                                   
help_wifi_config()                 
{                                  
		echo "             config"
		echo "                         -h                                                 --- Get help of Wifi Config Cmds"		                                                                                 
		echo "                         [ssid index]                                       --- display the config of ssid"
		echo "                                                                                index[if 4 SSIDs per radio ,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo ""                        
}                                              
                                               
help_wifi_status()                             
{                                              
		echo "             status"     
		echo "                         -h                                                 --- Get help of Wifi Status Cmds"		                                                                                 
		echo "                         common [ssid index]                                --- display the common status of ssid" 
		echo "                                                                                index[if 4 SSIDs per radio ,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo "                         sta {band value| ssid index [all | mac mac value]} --- display the assoclist sta info, value[a|b]"
		echo "                                                                                index[if 4 SSIDs per radio ,1~4 is 2.4G ,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G, 9~16 is 5G]"
		echo "                         network [band value]                               --- display the network status of band, value[a|b] "
		echo "                         channel [band value]                               --- display the change history of channel, value[a|b] "
		echo "                         connection [band value]                            --- display the sta connection, value[a|b] "
		echo ""                        
}                                              
                                               
help_wifi_debug()                              
{                                              
		echo "             debug"      
		echo "                         -h                                                 --- Get help of Wifi Debug Cmds"		                                                                                 
		echo "                         scan [band value] <enable|disable>                 --- enable/disable the acsd debug switch, value[a|b] "
		echo "                         scan [band value] ap start                         --- scan the neigborhood ap info , value[a|b] "
		echo "                         scan [band Value] channel start                    --- scan the channel info , value[a|b] "
		echo "                         wps <enable|disable>                               --- enable/disable the wps debug switch "
		echo "                         auth <enable|disable>                              --- enable/disable the auth debug switch "
		echo "                         adapter <enable|disable> [level]                   --- enable/disable the adapter of wifi debug switch and level,level[1-3]"
		echo "                         logfmt <0|1|2>                                     --- timpstamp format: 0:no timestamp 1:yyyy-mm-dd hh:mm:ss  2:second.microsecond"
		echo "                         unicastrate [band value] <rate>                    --- set the nrate, value[a|b] "
		echo "                         multicastrate [band value] <rate>                  --- set the mrate, value[a|b] "
		echo "                         interference  [band value] <mode>                  --- set the interference, value[a|b], mode[0-4] "
		echo "                         frameburst [band value] <enable|disable>           --- enable/disable frameburst, value[a|b] "
		echo "                         rts  [band value] <on|off|self>                    --- on/off/self rts, value[a|b] "
		echo "                         retry  [band value] lrlnumber srlnumber            --- value[a|b], lrlnumber[1,15], srlnumber[1,15] "
		echo "                         ampdu [band value] <enable|disable>                --- enable/disable the ampdu, value[a|b] "
		echo "                         ampdu_tid  [band value] <enable|disable>           --- enable/disable the ampdu_tid, tid[0-7] value[a|b] "
		echo "                         adaptivity [band value] <threshold>                --- set the CE threshold, value[a|b], threshold[-75,-55] "
		echo "                         amsdu [band value] <enable | disable>              --- enable/disable the amsdu, value[a|b]"
		echo "                         beaconmode [band value] <mode>                    --- set beacon chain mode, value[a|b], mode[0-2] "
		echo "                         edca [band value] { ap | sta } { vo | vi | be | bk } "
		echo "                             { aifs <aifs-value> | ecwmin <ecwmin-value> | " 
		echo "                             ecwmax <ecwmax-value> | txop <txop-value> |  "
		echo "                             ack-policy { normal | noack } }                --- set WMM Params "
		echo "                         history { band value {enable | disable} | ssid index {enable | disable} | duration seconds } "
		echo "                             --- enable/disalbe to collect the history info of the band[b|a] or users in ssid index[begin from 1] duration[seconds]"
		echo "                         history { show [{band value | ssid index | user mac } | start num1 | number num2]}"
		echo "                             --- show the history info, only show num2 items and skip the latest num1 items"
		echo "                             --- If band value[a|b] is specified, show the history info of the band and all users in this band"
		echo "                             --- If ssid index[begin from 1] is specified, show the history info of the band this ssid belongs to and all users in this ssid"
		echo "                             --- If user mac[user MAC address] is specified, show the history info of the band this user belongs to and this user"
		echo "                         history { release | output IP}         --- release/output the history info, output[IP]"
		echo "                         alarm  [ssid index] <enable|disable>           --- enable/disalbe the alram" 
		echo "                                                                            index[if 4 SSIDs per radio ,1~4 is 2.4G,5~8 is 5G;if 8 SSIDs per radio, 1~8 is 2.4G,9~16 is 5G]"
		echo "                         cca [band value] [regvalue] [inputvalue]       --- set the cca, value[a|b] regvalue[0x280|0x283|0x289|0x547] inputvalue[0x368d|0xa6]"
		echo ""
}

help_wifi()
{
                echo "         wifi"
                help_wifi_statistic
                help_wifi_config
                help_wifi_status
                help_wifi_debug 
}

help_other()
{
		echo "         mod print [module_name] [level] [0/1] --- Open the switch of specified module and level,level[0-3]"
		echo "         print on --- Open the printed switch of syslog"
		echo "         print off --- Close the printed switch of syslog"
		echo ""
}

help_datacard()
{
		echo "         datacard"
		echo "                 -h  --- Get help of datacard Module"
		echo "                 debug show --- show datacard module state" 
		echo ""
} 

help_ppm()
{
        echo "         ppm"
		echo "                 -h        --- Get help of ppm Module"
		echo "                 print on  --- Open the printed switch of ppm sm messages" 
		echo "                 print off --- Close the printed switch of ppm sm messages"  
		echo "                 stat display   --- get statistics of ppm info"
		echo "                 stat clear    --- clear statistics of ppm info"
		echo ""
}

help_sd()
{
		echo "         sd"
		echo "                 -h  --- Get help of datacard Module"
	    echo "                 print on  --- Open the printed switch of sd sm messages" 
		echo "                 print off --- Close the printed switch of sd sm messages" 
		echo "                 show state --- show sd module state" 
		echo ""
} 

help_pwr()
{
        echo "         pwr manage"
        echo "                 -h  --- Get help of pwr manage Module"
        echo "                 show state --- show pwr manage module state" 
        echo ""
} 

	if [ "$1" = "-h" ];then
		check_param 1 || exit $?
		echo "Usage:[module] [command] [option]"
		help_ploam
		help_optic
		help_led
		help_gpio
		help_i2c
		help_usb
		help_phy
		help_mpcp
		help_codec
		help_wifi
		help_other
		help_datacard
		help_ppm
		help_sd
		help_pwr
		help_lanploam
		help_lanomci
	
		exit 0
					
	elif [ "$1" = "lanploam" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_lanploam
			exit 0
		
		elif [ "$2" = "print" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				ampcmd ploam setprint 3 1 || exit $?
			elif [ "$3" = "off" ];then
				ampcmd ploam setprint 3 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
		fi

	elif [ "$1" = "lanomci" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_lanomci
			exit 0
		
		elif [ "$2" = "print" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				ampcmd print oltomcion || exit $?
			elif [ "$3" = "off" ];then
				ampcmd print oltomcioff || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
		fi

	elif [ "$1" = "ploam" ];then
		if [ -e /var/epon_up_mode.txt -o -e /var/ge_up_mode.txt ];then
			echo "ERROR::input para is not right!"
			exit 1
			
		elif [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_ploam
			exit 0
		
		elif [ "$2" = "print" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 0 0x2000300b 0xb 12 0 1 0 || exit $?
			elif [ "$3" = "off" ];then
				send_hlp 0 0x2000300b 0xb 12 0 0 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		elif [ "$2" = "alarm" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 0 0x2000300b 0xb 12 1 1 0 || exit $?
			elif [ "$3" = "off" ];then
				send_hlp 0 0x2000300b 0xb 12 1 0 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1			
			fi 

		elif [ "$2" = "err" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 0 0x2000300b 0xb 12 2 1 0 || exit $?
			elif [ "$3" = "off" ];then
				send_hlp 0 0x2000300b 0xb 12 2 0 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1			
			fi 

		elif [ "$2" = "omci" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 0 0x2000300b 0xb 12 3 1 0 || exit $?
			elif [ "$3" = "off" ];then
				send_hlp 0 0x2000300b 0xb 12 3 0 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1				
			fi 

		elif [ "$2" = "stat" ];then
			check_param 2 || exit $?
			send_hlp 0 0x2000300e 0xe 4 1 || exit $?

		elif [ "$2" = "clear" ];then
			check_param 3 || exit $?
			if [ "$3" = "stat" ];then
				send_hlp 0 0x2000300d 0xd 4 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1					
			fi

		elif [ "$2" = "bwmap" ];then
			check_param 2 || exit $?
			send_hlp 0 0x2000300c 0xc 0 0 || exit $?
			
		elif [ "$2" = "show" ];then
			check_param 3 || exit $?
			if [ "$3" = "log" ];then
				send_hlp 0 0x20003016 0x16 4 0 || exit $?
				sleep 1
				exit 0
			else
				echo "ERROR::input para is not right!"
				exit 1					
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
		
	elif [ "$1" = "datacard" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_datacard
			exit 0
		
		elif [ "$2" = "debug" ];then		  
		    if [ "$3" = "show" ];then
		       check_param 3 || exit $?
			   send_hlp 0 0x20000000 0 12 0x17 1 1 || exit $?
               send_hlp 0 0x20017022 0x22 8 0xFFFFFFFF 0 || exit $?
			   send_hlp 0 0x20000000 0 12 0x17 1 0 || exit $?
            else
			   echo "ERROR::input para is not right!"
			   exit 1
		    fi
		  
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	elif [ "$1" = "optic" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_optic
			exit 0
		
		elif [ "$2" = "display" ];then
			check_param 3 || exit $?
			if [ "$3" = "reg" ];then
				send_hlp 0 0x20005009 0x9 24 0 1 0 0 0 0x70  || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1					
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	elif [ "$1" = "led" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_led
			exit 0
		
		elif [ "$2" = "display" ];then
			if [ "$3" = "status" ];then
				if [ "$4" = "" -o "$5" = "" ];then
					echo "ERROR::input para is not right!"
					exit 1
				fi
				check_param 5 || exit $?
				send_hlp 120 0x20008004 0x4 12 1 "$4" "$5" || exit $?	
			else
					echo "ERROR::input para is not right!"
					exit 1					
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	elif [ "$1" = "gpio" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_gpio
			exit 0

		elif [ "$2" = "display" ];then
			if [ "$3" = "value" ];then
				check_param 4 || exit $?
				send_hlp 100 0x20012004 4 4 "$4" || exit $?

			else
				echo "ERROR::input para is not right!"
				exit 1					
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	elif [ "$1" = "i2c" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_i2c
			exit 0

		elif [ "$2" = "read" ];then
			if [ "$3" = "" -o "$4" = "" ];then
				echo "ERROR::input para is not right!"
				exit 1
			fi
			check_param 4 || exit $?
			ishex $4
			if [ $? -ne 1 ]; then
				echo "ERROR::input para is not right!"
				exit 1
			fi

			if [ "$3" = "A0" ];then
				send_hlp 100 0x20013000 0 20 0x3 0x500000 0x"$4" 0x10001 0x0 || exit $?
			elif [ "$3" = "A2" ];then
				send_hlp 100 0x20013000 0 20 0x3 0x510000 0x"$4" 0x10001 0x0 || exit $?
			elif [ "$3" = "A4" ];then
				send_hlp 100 0x20013000 0 20 0x3 0x520000 0x"$4" 0x10001 0x0 || exit $?
			elif [ "$3" = "A6" ];then
				send_hlp 100 0x20013000 0 20 0x3 0x530000 0x"$4" 0x10001 0x0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1					
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	elif [ "$1" = "usb" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_usb
			exit 0

		elif [ "$2" = "display" ];then
			if [ "$3" = "status" ];then
				check_param 3 || exit $?
				send_hlp 150 0x2000c000 0x0 0 || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
			
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
		
	elif [ "$1" = "codec" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_codec
			exit 0
		
		elif [ "$2" = "display" ];then
			if [ "$3" = "global" -a "$4" = "info" ];then
				check_param 5 || exit $?
				send_hlp 0 0x20006304 4 4 "$5" || exit $?
				send_hlp 80 0x20006307 0x7 4 "$5" || exit $?
				sleep 0.05
				exit 0
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
				
	elif [ "$1" = "phy" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_phy
			exit 0
		elif [ "$2" = "display" ];then
			if [ "$3" = "debug" ];then
				if [ "$4" = "info" ];then
					check_param 4 || exit $?
				    send_hlp 200 0x20037020 0x20 20 0 0 1 0 0 || exit $?
				else 
					echo "ERROR::input para is not right!"
					exit 1
				fi
				
			elif [ "$3" = "extphy" ];then
				if [ "$4" = "info" ];then
					send_hlp 0 0x2000900b 0xb 20 2 0 0 0 0 || exit $?
					sleep 1.5
					exit 0
				else 
					echo "ERROR::input para is not right!"
					exit 1
				fi
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi	
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
	elif [ "$1" = "mpcp" ];then
		if [ -e /var/gpon_up_mode.txt -o -e /var/ge_up_mode.txt ];then
			echo "ERROR::input para is not right!"
			exit 1
			
		elif [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_mpcp
			exit 0
		
		elif [ "$2" = "switch" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 1000 0x20031006 0x6 8 0 1 || exit $?
				send_hlp 1000 0x20031006 0x6 8 1 1 || exit $?
				send_hlp 1000 0x20031006 0x6 8 2 1 || exit $?
				
			elif [ "$3" = "off" ];then
				send_hlp 1000 0x20031006 0x6 8 0 0 || exit $?
				send_hlp 1000 0x20031006 0x6 8 1 0 || exit $?
				send_hlp 1000 0x20031006 0x6 8 2 0 || exit $?
			
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi

		elif [ "$2" = "display" ];then
			if [ "$3" = "statistics" ];then
				check_param 3 || exit $?
				send_hlp 1000 0x20031007 0x07 8 0 0|| exit $?
                		send_hlp 1000 0x20031007 0x07 8 1 0|| exit $?				
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
			
		elif [ "$2" = "show" ];then
			if [ "$3" = "log" ];then
				check_param 3 || exit $?
				send_hlp 1000 0x20031007 0x07 8 2 0|| exit $?
				sleep 1	
				exit 0			
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi		
		
	elif [ "$1" = "wifi" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_wifi_statistic
			help_wifi_config
			help_wifi_status
			help_wifi_debug
			exit 0			
		elif [ "$3" = "-h" ];then
			check_param 3 || exit $?
			if [ "$2" = "statistic" ];then
				help_wifi_statistic
				exit 0
			elif [ "$2" = "config" ];then
				help_wifi_config
				exit 0
			elif [ "$2" = "status" ];then
				help_wifi_status
				exit 0
			elif [ "$2" = "debug" ];then
				help_wifi_debug
				exit 0
			fi			
		elif [ -e /bin/hw_ldsp_wifi_cmd ];then
				hw_ldsp_wifi_cmd $@ || exit $?
		else
			echo "ERROR::Command is not right!"
			exit 1	
		fi

	elif [ "$1" = "mod" ];then
		if [ "$2" = "print" ];then
			check_param 5 || exit $?
			if [ "$3" = "hlp" ];then
				send_hlp 0 0x20000000 0 12 0 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				    mid set 110 "$5"
                fi					
			elif [ "$3" = "lsw" ];then
				send_hlp 0 0x20000000 0 12 79 "$4" "$5" || exit $?
                exit 1
			elif [ "$3" = "gmac" ];then
                if [ "$4" = "0" ];then
                    mid set 111 "$5"
				fi
                exit 1
			elif [ "$3" = "ploam" ];then
				send_hlp 0 0x20000000 0 12 3 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				    mid set 112 "$5"
				fi
			elif [ "$3" = "dev" ];then
				send_hlp 0 0x20000000 0 12 4 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				    mid set 113 "$5"
				fi
			elif [ "$3" = "optic" ];then
				send_hlp 0 0x20000000 0 12 5 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				    mid set 114 "$5"
				fi
			elif [ "$3" = "codec" ];then
				send_hlp 0 0x20000000 0 12 6 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				   mid set 115 "$5"
				fi
			elif [ "$3" = "key" ];then
				send_hlp 0 0x20000000 0 12 7 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				   mid set 116 "$5"
				fi
			elif [ "$3" = "led" ];then
				send_hlp 0 0x20000000 0 12 8 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				   mid set 117 "$5"
				fi
			elif [ "$3" = "flash" ];then
				send_hlp 0 0x20000000 0 12 9 "$4" "$5" || exit $?
			elif [ "$3" = "battery" ];then
				send_hlp 0 0x20000000 0 12 10 "$4" "$5" || exit $?
			elif [ "$3" = "rf" ];then
				send_hlp 0 0x20000000 0 12 11 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				   mid set 120 "$5"
				fi
			elif [ "$3" = "usb" ];then
				send_hlp 0 0x20000000 0 12 12 "$4" "$5" || exit $?
				if [ "$4" = "0" ];then
				   mid set 121 "$5"
				fi
			elif [ "$3" = "phy" ];then
				send_hlp 0 0x20000000 0 12 13 "$4" "$5" || exit $?
			elif [ "$3" = "dsp" ];then
				send_hlp 0 0x20000000 0 12 14 "$4" "$5" || exit $?
			elif [ "$3" = "emac" ];then
                if [ "$4" = "0" ];then
                   mid set 124 "$5"
				fi
                exit 1
			elif [ "$3" = "oam" ];then
				send_hlp 0 0x20000000 0 12 16 "$4" "$5" || exit $?
			elif [ "$3" = "wifi" ];then
				send_hlp 0 0x20000000 0 12 17 "$4" "$5" || exit $?
			elif [ "$3" = "gpio" ];then
				send_hlp 0 0x20000000 0 12 18 "$4" "$5" || exit $?
			elif [ "$3" = "i2c" ];then
				send_hlp 0 0x20000000 0 12 19 "$4" "$5" || exit $?
			elif [ "$3" = "mdio" ];then
                exit 1
			elif [ "$3" = "uart" ];then
				send_hlp 0 0x20000000 0 12 21 "$4" "$5" || exit $?
			elif [ "$3" = "highway" ];then
				send_hlp 0 0x20000000 0 12 22 "$4" "$5" || exit $?
			elif [ "$3" = "e2prom" ];then
				send_hlp 0 0x20000000 0 12 23 "$4" "$5" || exit $?
			elif [ "$3" = "spi" ];then
				send_hlp 0 0x20000000 0 12 24 "$4" "$5" || exit $?
			elif [ "$3" = "sim" ];then
				send_hlp 0 0x20000000 0 12 25 "$4" "$5" || exit $?
				send_hlp 0 0x20019004 0x4 0 || exit $?
			elif [ "$3" = "serial" ];then
				send_hlp 0 0x20000000 0 12 26 "$4" "$5" || exit $?
			elif [ "$3" = "acl" ];then
                exit 1
			elif [ "$3" = "napt" ];then
                exit 1
			elif [ "$3" = "nfc" ];then
			  	send_hlp 0 0x20000000 0 12 32 "$4" "$5" || exit $?
			elif [ "$3" = "extlsw" ];then
                exit 1
			elif [ "$3" = "common" ];then
				send_hlp 0 0x20000000 0 12 34 "$4" "$5" || exit $?
			elif [ "$3" = "topo" ];then
				send_hlp 0 0x20000000 0 12 44 "$4" "$5" || exit $?	
			elif [ "$3" = "qos" ];then
			    if [ "$4" = "0" ];then
				   mid set 136 $5	
                fi				
			elif [ "$3" = "poe" ];then
				send_hlp 0 0x20000000 0 12 78 "$4" "$5" || exit $?
			elif [ "$3" = "slave" ];then
				send_hlp 0 0x20000000 0 12 83 "$4" "$5" || exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi
		
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
	elif [ "$1" = "print" ];then
		if [ "$2" = "on" -o "$2" = "off" ]; then
		   check_param 2 || exit $?
		    if [ "$2" = "on" ];then
			    mid set 110 1
			    mid set 122 1
		    elif [ "$2" = "off" ];then
			    mid set 110 0
			    mid set 122 0
		    fi			
		else
			echo "ERROR::input para is not right!"
			exit 1
		fi
	elif [ "$1" = "ppm" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_ppm
			exit 0
		
		elif [ "$2" = "print" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp 0 0x2001b005 0x5 12 1 1 0|| exit $?
			elif [ "$3" = "off" ];then
				send_hlp 0 0x2001b005 0x5 12 1 0 0|| exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		elif [ "$2" = "stat" ];then
		    check_param 3 || exit $?
			if [ "$3" = "clear" ];then
				send_hlp 0 0x2001b005 0x5 12 0 0 0|| exit $?			
			elif [ "$3" = "display" ];then
				send_hlp 0 0x2001b005 0x5 12 0 1 0|| exit $?
				sleep 0.05
				exit 0
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		else
			echo "ERROR::input para is not right!"
			exit 1
		fi 
		
       
	elif [ "$1" = "sd" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_sd
			exit 0
		
		elif [ "$2" = "print" ];then
			check_param 3 || exit $?
			if [ "$3" = "on" ];then
				send_hlp_check  0  0x20024003  3  4 1|| exit $?
			elif [ "$3" = "off" ];then
				send_hlp_check  0  0x20024003  3  4 0|| exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		elif [ "$2" = "show" ];then
		    check_param 3 || exit $?
			if [ "$3" = "state" ];then
				send_hlp_check 0 0x20024004  4  0|| exit $?			
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		else
			echo "ERROR::input para is not right!"
			exit 1
		fi 
	elif [ "$1" = "pwr" ];then
		if [ "$2" = "-h" ];then
			check_param 2 || exit $?
			help_pwr
			exit 0

		elif [ "$2" = "show" ];then
		    check_param 3 || exit $?
			if [ "$3" = "state" ];then
				send_hlp_check 200 0x20004062 0x62 0|| exit $?
			else
				echo "ERROR::input para is not right!"
				exit 1
			fi 

		else
			echo "ERROR::input para is not right!"
			exit 1
		fi

	else 
		echo "ERROR::Command is not right!"
		exit 1
	fi
